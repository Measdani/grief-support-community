# Supabase Configuration
# Get these values from your Supabase project settings
# https://app.supabase.com/project/_/settings/api

NEXT_PUBLIC_SUPABASE_URL=your-project-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Stripe Configuration
# Get these values from your Stripe Dashboard
# https://dashboard.stripe.com/apikeys

# Secret key for server-side Stripe API calls (starts with sk_test_ or sk_live_)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Webhook signing secret for verifying webhook authenticity (starts with whsec_)
# Get this after creating a webhook endpoint at https://dashboard.stripe.com/webhooks
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Admin Configuration
NEXT_PUBLIC_ADMIN_EMAILS=admin@example.com
